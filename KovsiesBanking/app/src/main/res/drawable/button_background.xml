<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/buttonColorEditProfilePressed" /> <!-- Change color for pressed state -->
            <corners android:radius="16dp"/> <!-- Rounded corners -->
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/buttonColorEditProfilee" /> <!-- Default button color -->
            <corners android:radius="16dp"/> <!-- Rounded corners -->
        </shape>
    </item>
</selector>
